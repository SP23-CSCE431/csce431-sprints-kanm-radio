<p style="color: green"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>NetID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>UIN</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Shirt</th>
      <th>Strikes</th>
      <th>Points</th>
      <th>DJ</th>
      <th>Edit this member</th>
    </tr>
  </thead>
  
  <tbody>
      <% @members.each do |member| %>
        <tr>
          <td><%= member.netid %></td>
          <td><%= member.firstname %></td>
          <td><%= member.lastname %></td>
          <td><%= member.uin %></td>
          <td><%= member.email %></td>
          <td><%= member.phone %></td>
          <td><%= member.shirt %></td>
          <td><%= member.strikes %></td>
          <td><%= member.points %></td>
          <td><%= member.dj %></td>
          <td><%= link_to "Show this member", member %></td>
        <tr>
      <% end %>
  </tbody>
</table>



<%= link_to "New member", new_member_path %>
<br>
<%= link_to "Show index", shows_path %>
<br>
<%= link_to "Show host index", show_hosts_path %>
